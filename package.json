{
  "name": "cupdb",
  "version": "1.0.4",
  "description": "a text based key-value database for node.js",
  "main": "./src/index.js",
  "scripts": {
    "test": "ava -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oldj/cupdb.git"
  },
  "author": "oldj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oldj/cupdb/issues"
  },
  "homepage": "https://github.com/oldj/cupdb#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "mkdirp": "^0.5.1",
    "ramda": "^0.25.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js",
      "test/*.js",
      "!**/node_modules/**/*.*"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false
  }
}
